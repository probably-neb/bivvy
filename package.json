{
  "name": "paypals",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "sso": "aws sso login --sso-session=paypals",
    "update:.env.local": "npm sst secrets list env > .env.local"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.10.5",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0",
    "sst": "2.40.3",
    "typescript": "^5.3.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "pnpm": {
    "overrides": {
      "@kobalte/core": "link:./packages/kobalte/packages/core"
    }
  }
}
